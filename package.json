{
  "name": "ecopal",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "ibrahimcaj",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android --local --profile production --clear-cache",
    "build:ios": "eas build --platform android --local --clear-cache",
    "build:android debug": "eas build --platform android --profile debug --local --clear-cache",
    "build:ios debug": "eas build --platform android --profile debug --local --clear-cache",
    "clean": "cd android && ./gradlew clean && cd ..",
    "hash": "cd android && keytool -list -alias androiddebugkey -keystore ./app/debug.keystore -v",
    "format": "prettier --log-level warn --write \"./**/*.{tsx,ts,jsx,js}\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
